   "source": [
    "p = sns.relplot(data=ppa_price_long, \n",
    "    x=\"ppa_execution_date\", \n",
    "    y = \"price\", \n",
    "    size = \"capacity_mw\", \n",
    "    hue = \"region_cat\",\n",
    "    palette = colormap,\n",
    "    ec=ppa_price_long[\"region_cat\"].map(colormap),\n",
    "    fc=\"none\",\n",
    "    sizes = (3, 550))\n",
    "\n",
    "p.set(xlabel=None, ylabel=\"PPA Price (2020 $/MWh)\")\n",
    "\n",
    "p.legend.remove()\n",
    "p.fig.legend(handles=p.legend.legendHandles[:11])"
   ]
